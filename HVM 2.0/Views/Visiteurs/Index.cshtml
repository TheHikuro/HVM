@model IEnumerable<HVM_2._0.Models.Creneau>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

    <table class="table">
        <div>
            <td>
                @Html.DropDownList("Creneau", new List<SelectListItem>
           {
               new SelectListItem { Text = "IT", Value = "1", Selected=true},
               new SelectListItem { Text = "Account", Value = "2"},
               new SelectListItem { Text = "Sales", Value = "3"},
               new SelectListItem { Text = "HR", Value = "4"}
           }, "Select Department")
            </td>
            <input type="submit" class="btn btn-default" id="next" value="Suivant" onclick="" />
        </div>

    </table>


@foreach (var item in Model)
{
    <div>
        <ul>
            @item.date
        </ul>
        <ul>
            @item.heure
        </ul>
    </div>
}

