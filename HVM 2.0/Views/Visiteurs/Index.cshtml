@model IEnumerable<HVM_2._0.Models.Creneau>
@{
    ViewBag.Title = "Index";
}



<h2>Index</h2>

    <select name="Creneaux">
        @foreach (var item in Model)
        {
            <option>@item.date</option>

        }
        
    </select>


    <input type="submit" class="btn btn-default" id="next" value="Suivant" onclick="@("window.location.href='" + @Url.Action("Inscription", "Visiteurs") + "'");"/>
