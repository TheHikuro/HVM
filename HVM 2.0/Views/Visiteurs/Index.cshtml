<link rel="Shortcut Icon" href="favicon.ico" type="image/x-icon" />
@model IEnumerable<HVM_2._0.Models.Creneau>
@{
    ViewBag.Title = "Choix Créneau";
}
<h2 style="text-align:center">Choix d'un créneau</h2>
<hr style="border-top: 1px solid #000000" />

<form method="post" action="/Visiteurs/Index/">
    <select name="Creneaux" style="font-size:20px">
        @foreach (var item in Model)
        {
            <option value="@item.date">@item.date</option>
        }
    </select>
    <input type="submit" class="btn btn-default" id="next" value="Suivant" style="background-color:palegreen;margin-left:15px"/>
</form>
    


    
